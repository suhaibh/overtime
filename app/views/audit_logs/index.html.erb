<h2>Audit Logs</h2>

<table class="table table-striped table-hover table-bordered">
	<thead>
		<tr>
			<th>Audit Log #</th>
			<th>Name</th>
			<th>Status</th>
			<th>Week Starting</th>
			<th>Confirmed At</th>
		</tr>
	</thead>
	<tbody id="audit-logs">
		<%= render @audit_logs %>
	</tbody>
</table>

<div id="paginator">
	<%= paginate @audit_logs, remote: true %>
</div>

